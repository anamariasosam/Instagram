<tr>
  <td><%= order.id %></td>
  <td><%= order.status %></td>
  <td><%= time_ago_in_words order.created_at %></td>
  <td>
    <% if current_user.type == 'Customer' %>
      <%= order.store.name %>
    <% else %>
      <%= order.customer.full_name %>
    <% end %>
  </td>
  <td><%= order.product.product_name %></td>
  <td>
    <% if current_user.type == 'Customer' %>
      <a href="tel:<%= order.store.phone_number %>">
        <%= order.store.phone_number %>
      </a>
      <br>
      <a href="mailTo:<%= order.store.email %>">
        <%= order.store.email %>
      </a>
    <% else %>
      <a href="tel:<%= order.customer.phone_number %>">
        <%= order.customer.phone_number %>
      </a>
      <br>
      <a href="mailTo:<%= order.customer.email %>">
        <%= order.customer.email %>
      </a>
    <% end %>
  </td>
  <td> Print </td>
</tr>
